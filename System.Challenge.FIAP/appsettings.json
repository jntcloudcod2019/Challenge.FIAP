{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "Urls": "http://localhost:7070",
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Database": {
    "Server": "",
    "ServerPublic": "gondola.proxy.rlwy.net",
    "Port": "1433",
    "PortPublic": "32468",
    "Username": "sa",
    "Password": "",
    "Name": "master",
    "UsePublicServer": false
  },
  "Railway": {
    "MSSQL_SERVER": "microsoftsql.railway.internal",
    "MSSQL_SERVER_PUBLIC": "gondola.proxy.rlwy.net",
    "MSSQL_TCP_PORT": "1433",
    "MSSQL_TCP_PORT_PUBLIC": "32468",
    "MSSQL_USERNAME": "sa",
    "MSSQL_SA_PASSWORD": "",
    "MSSQL_DATABASE": "master"
  },
  "JwtSettings": {
    "SecretKey": "",
    "Issuer": "",
    "Audience": "",
    "ExpirationInMinutes": 240
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  }
}
